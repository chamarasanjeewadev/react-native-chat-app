{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "jsx": "preserve",
    "module": "NodeNext",
    "strict": false
  },
  "paths": {
    "src/*": ["src/*"],
    "assets/*": ["src/assets/*"],
    "screens/*": ["src/screens/*"],
    "components/*": ["src/components/*"],
    "navigators/*": ["src/navigators/*"],
    "services/*": ["src/services/*"],
    "utils/*": ["src/utils/*"],
    "store/*": ["src/stores/*"]
  }
}
