{
  "name": "milai.mobilepp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "setDevelopment": "ENVFILE=.env.development",
    "setStaging": "ENVFILE=.env.staging",
    "setProduction": "ENVFILE=.env.production",
    "ios:Pod:Reset": "cd ios && pod deintegrate && pod setup && pod install",
    "ios:clean": "yarn node:clean &&  cd ios && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && yarn ios:Pod:Reset",
    "ios:Pod:install": "cd ios && pod install && cd ..",
    "ios:bundle:assets": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "ios:dev": "yarn setDevelopment react-native run-ios --mode=Debug --scheme \"development\"",
    "ios:dev-release": "yarn ios:clean && yarn setDevelopment  react-native run-ios --mode=Release --scheme \"development\"",
    "ios:staging": "yarn setStaging  react-native run-ios --mode=Debug --scheme \"staging\"",
    "ios:staging-release": "yarn ios:clean && yarn setStaging  react-native run-ios --mode=Release --scheme \"staging\"",
    "ios:prod": "yarn setProduction  react-native run-ios --mode=Debug --scheme \"production\"",
    "ios:prod-release": " yarn ios:clean && yarn ios:bundle:assets && yarn setProduction  react-native run-ios --mode=Release --scheme \"production\"",
    "android:dev": "yarn setDevelopment  react-native run-android --mode=developmentDebug --appIdSuffix=development",
    "android:dev-release": "yarn setDevelopment  react-native run-android --mode=developmentRelease--appIdSuffix=development",
    "android:staging": "yarn setStaging  react-native run-android --mode=stagingDebug --appIdSuffix=staging",
    "android:staging-release": "yarn setStaging  react-native run-android --mode=stagingRelease --appIdSuffix=staging",
    "android:prod": "yarn setProduction  react-native run-android --mode=productionDebug --appIdSuffix=production",
    "android:prod-release": "yarn setProduction  react-native run-android --mode=productionRelease --appIdSuffix=production",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@lukemorales/query-key-factory": "^1.3.2",
    "@react-native-community/checkbox": "^0.5.17",
    "@react-native-community/slider": "^4.5.0",
    "@react-native-firebase/app": "^18.8.0",
    "@react-native-firebase/auth": "^18.8.0",
    "@react-native-google-signin/google-signin": "^11.0.0",
    "@react-native-picker/picker": "^2.6.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@stripe/stripe-react-native": "^0.36.0",
    "@tanstack/query-sync-storage-persister": "^5.17.19",
    "@tanstack/react-query": "^5.14.0",
    "@tanstack/react-query-persist-client": "^5.17.19",
    "@types/react-native-vector-icons": "^6.4.18",
    "axios": "^1.6.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "expo": "^50.0.0",
    "expo-av": "^13.4.1",
    "i18next": "^23.8.2",
    "moti": "^0.27.2",
    "nativewind": "4.0.13",
    "react": "18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-hook-form": "^7.50.0",
    "react-i18next": "^14.0.1",
    "react-native": "^0.73.4",
    "react-native-app-auth": "^7.1.0",
    "react-native-bootsplash": "^5.4.0",
    "react-native-config": "^1.5.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^2.11.0",
    "react-native-picker-select": "^9.0.1",
    "react-native-reanimated": "^3.6.2",
    "react-native-safe-area-context": "^4.8.1",
    "react-native-screens": "^3.29.0",
    "react-native-snackbar": "^2.6.2",
    "react-native-svg": "^14.1.0",
    "react-native-track-player": "^4.0.1",
    "react-native-vector-icons": "^10.0.3",
    "react-native-webview": "^13.8.1",
    "tailwind-merge": "^2.2.1",
    "yup": "^1.3.3",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.3",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react-native-svg-transformer": "^1.3.0",
    "react-query-native-devtools": "^4.0.0",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "^5.1.3",
    "reactotron-react-query": "^1.0.2",
    "tailwindcss": "^3.4.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
